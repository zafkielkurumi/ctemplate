{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "SchematicsFlutter",
    "title": "Flutter Schema",
    "type": "object",
    "properties": {
      "name": {
        "description": "文件名称",
        "type": "string",
        "minLength": 1,
        "x-prompt": "输入文件名称"
      },
      "state": {
        "default": true,
        "type": "boolean",
        "x-prompt": "是否为StatefulWidget?"
      },
      "isPage": {
        "default": true,
        "type": "boolean",
        "x-prompt": "是否为页面,y是,页面会添加ff_route"
      }
     },
    "required": [
      "name"
    ]
  }